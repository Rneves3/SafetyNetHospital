	public DaysOfMonth: nat * nat1 -> nat1
		DaysOfMonth(y,m) == 
			if m = 4 or m = 6 or m = 9 or m = 11 then 30
			else if m <> 2 then 31
			else if IsLeap(y) then 29
			else 28;
			
	public IsLeap: nat -> bool
		IsLeap(y) ==
			if (y mod 4 = 0 and y mod 100 <> 0) or
				(y mod 400 = 0) then true
			else false;	
