	public Date::
		year: nat
		month: nat1
		day: nat1
		hour: nat
		minute: nat
		inv mk_Date(year,month,day,hour,minute) == month <= 12 and 
			day <= DaysOfMonth(year,month) and hour <= 23	and minute <= 59;
				
